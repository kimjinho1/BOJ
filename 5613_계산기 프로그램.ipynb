   "source": [
    "res = int(input())\n",
    "while 1:\n",
    "    op = input()\n",
    "    if op == '=':\n",
    "        break\n",
    "    n = int(input())\n",
    "    if op == '+': res += n;\n",
    "    elif op == '-': res -= n;\n",
    "    elif op == '*': res *= n;\n",
    "    elif op == '/': res //= n;\n",
    "print(res)"
   ]
  },
