   "source": [
    "s, k = map(int, input().split())\n",
    "print((s//k)**(k-s%k) * (s//k+1)**(s%k))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "id": "creative-coalition",
   "metadata": {
    "ExecuteTime": {
     "end_time": "2021-02-20T16:08:33.216210Z",
     "start_time": "2021-02-20T16:08:32.559963Z"
    }
   },
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "10 3\n",
      "36\n"
     ]
    }
   ],
   "source": [
    "s, k = map(int, input().split())\n",
    "li = [s//k for _ in range(k)]\n",
    "for i in range(s%k):\n",
    "    li[i] += 1\n",
    "res = 1\n",
    "for n in li:\n",
    "    res *= n\n",
    "print(res)"
   ]
  },
