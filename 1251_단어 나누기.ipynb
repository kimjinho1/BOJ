   "source": [
    "s = input()\n",
    "li = []\n",
    "for i in range(len(s)-2):\n",
    "    for j in range(i+1, len(s)-1):\n",
    "        for k in range(j+1, len(s)):\n",
    "            t = s[:j][::-1] + s[j:k][::-1] + s[k:][::-1]\n",
    "            li.append(t)\n",
    "print(min(li))"
   ]
