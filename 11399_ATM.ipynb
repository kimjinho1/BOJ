    "# 바로 통과했다.\n",
    "N = int(input())\n",
    "li = list(map(int, input().split()))\n",
    "li.sort()\n",
    "ans = 0\n",
    "for i in range(N):\n",
    "    ans += sum(li[:i+1])\n",
    "print(ans)"
