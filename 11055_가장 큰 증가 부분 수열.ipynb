   "source": [
    "from copy import deepcopy\n",
    "\n",
    "N = int(input())\n",
    "li = list(map(int, input().split()))\n",
    "dp = deepcopy(li)\n",
    "for i in range(1, N):\n",
    "    for j in range(i):\n",
    "        if li[j] < li[i]:\n",
    "            dp[i] = max(dp[i], li[i]+dp[j])\n",
    "print(max(dp))"
   ]
