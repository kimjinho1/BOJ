    "for _ in range(int(input())):\n",
    "    a, b = map(int, input().split())\n",
    "    a, b = max(a, b), min(a, b)\n",
    "    s = a*b\n",
    "    \n",
    "    while(b != 0):\n",
    "        n = a%b\n",
    "        a = b\n",
    "        b = n\n",
    "    \n",
    "    print(s//a)"
