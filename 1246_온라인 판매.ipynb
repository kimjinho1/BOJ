   "source": [
    "N, M = map(int, input().split())\n",
    "li = sorted([int(input()) for _ in range(M)])\n",
    "max_p = max_b = 0\n",
    "for i in range(M):\n",
    "    t = li[i] * ((M-i) if M-i <= N else N)\n",
    "    if max_b < t:\n",
    "        max_b = t\n",
    "        max_p = li[i]\n",
    "print(max_p, max_b)"
   ]
