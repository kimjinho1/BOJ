   "source": [
    "n = int(input())\n",
    "li = list(map(int, input().split()))\n",
    "ans = [0]*n\n",
    "for i in range(n):\n",
    "    cnt = 0\n",
    "    for j in range(n):\n",
    "        if ans[j] != 0 and i+1 > ans[j]:\n",
    "            continue\n",
    "        if cnt == li[i]:\n",
    "            ans[j] = i+1\n",
    "            break\n",
    "        cnt += 1\n",
    "print(*ans)"
   ]
