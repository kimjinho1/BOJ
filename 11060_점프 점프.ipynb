   "source": [
    "N = int(input())\n",
    "li = list(map(int, input().split()))\n",
    "dp = [N+1]*N\n",
    "dp[0] = 0\n",
    "for i in range(N):\n",
    "    for j in range(1, li[i]+1):\n",
    "        if i+j >= N:\n",
    "            break\n",
    "        dp[i+j] = min(dp[i+j], dp[i]+1)\n",
    "print(dp[N-1] if dp[N-1] != N+1 else -1)"
   ]
