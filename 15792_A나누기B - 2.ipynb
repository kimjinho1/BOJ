    "a, b = map(int, input().split())\n",
    "print(a//b, end='')\n",
    "if a%b:\n",
    "    i = 0\n",
    "    print('.', end='')\n",
    "    while a%b and i < 1000: \n",
    "        a = a%b * 10\n",
    "        i += 1\n",
    "        print(a//b, end='')"
