   "source": [
    "for case in range(int(input())):\n",
    "    t = list(map(int, input().split()))\n",
    "    n, li = t[0], sorted(t[1:])\n",
    "    Min = li[0]; Max = li[-1]\n",
    "    g = max([li[i+1]-li[i] for i in range(n-1)])\n",
    "    print(\"Class\", case+1)\n",
    "    print(f\"Max {Max}, Min {Min}, Largest gap {g}\")"
   ]
