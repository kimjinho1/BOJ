    "n = input()\n",
    "check = n\n",
    "cnt = 0\n",
    "while(True):\n",
    "    if len(n) == 1:\n",
    "        n = str(int(n)*10 + int(n))\n",
    "    else:\n",
    "        n = str(int(n[1])*10 + int(str(int(n[0])+int(n[1]))[-1]))\n",
    "    cnt += 1\n",
    "    if n == check:\n",
    "        break\n",
    "print(cnt)"
