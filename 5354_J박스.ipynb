    "for _ in range(int(input())):\n",
    "    n = int(input())\n",
    "    if n < 3:\n",
    "        for i in range(n):\n",
    "            print('#'*n)\n",
    "        print()\n",
    "    else:\n",
    "        print('#'*n)\n",
    "        for i in range(n-2):\n",
    "            print('#' + 'J'*(n-2) + '#')\n",
    "        print('#'*n, '\\n')"
