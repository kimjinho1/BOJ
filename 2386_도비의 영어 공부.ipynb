    "while 42:\n",
    "    t = input()\n",
    "    if t == '#':\n",
    "        break\n",
    "    li = t.split()\n",
    "    alpha_upper = li[0].upper()\n",
    "    alpha_lower = li[0].lower()\n",
    "    s = \" \".join(li[1:])\n",
    "    cnt = s.count(alpha_upper) + s.count(alpha_lower)\n",
    "    print(\"{0} {1}\".format(alpha_lower, cnt))"
