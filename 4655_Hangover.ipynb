   "source": [
    "while 1:\n",
    "    n = float(input())\n",
    "    if n == 0:\n",
    "        break\n",
    "    i = 2\n",
    "    t = 0\n",
    "    while t < n:\n",
    "        t += 1/i\n",
    "        i += 1\n",
    "    print(f\"{i-2} card(s)\")"
   ]
