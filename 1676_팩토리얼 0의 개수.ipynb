   "source": [
    "n = int(input())\n",
    "t = n\n",
    "n2, n5 = 0, 0\n",
    "while(t != 0):\n",
    "    t //= 2\n",
    "    n2 += t\n",
    "while(n != 0):\n",
    "    n //= 5\n",
    "    n5 += n\n",
    "print(min(n2, n5))"
   ]
