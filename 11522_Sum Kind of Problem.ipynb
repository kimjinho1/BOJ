    "for _ in range(int(input())):\n",
    "    k, N = map(int, input().split())\n",
    "    s1 = N*(N+1)//2\n",
    "    s2 = int((N*2)*(N/2))\n",
    "    s3 = int((N*2+2)*(N/2))\n",
    "    print(k, s1, s2, s3)"
