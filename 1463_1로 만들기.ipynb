    "X = int(input())\n",
    "cnt = 0\n",
    "li = [X]\n",
    "\n",
    "while(1 not in li):\n",
    "    t = []\n",
    "    for x in li:\n",
    "        if x % 3 == 0:\n",
    "            t.append(x//3)\n",
    "        elif x % 2 == 0:\n",
    "            t.append(x//2)\n",
    "        t.append(x-1)\n",
    "    li = t\n",
    "    cnt += 1\n",
    "print(cnt)"
   ]
