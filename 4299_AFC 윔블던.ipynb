   "source": [
    "s, m = map(int, input().split())\n",
    "if  s+m < 0 or s-m < 0 or (s + m) % 2:\n",
    "    print(-1)\n",
    "else:\n",
    "    a = (s + m) // 2\n",
    "    b = s - a\n",
    "    print(max(a, b), min(a, b))"
