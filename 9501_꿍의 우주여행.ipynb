   "source": [
    "for _ in range(int(input())):\n",
    "    N, D = map(int, input().split())\n",
    "    cnt = 0\n",
    "    for i in range(N):\n",
    "        v, f, c = map(int, input().split())\n",
    "        if v * f/c >= D:\n",
    "            cnt += 1\n",
    "    print(cnt)"
   ]
