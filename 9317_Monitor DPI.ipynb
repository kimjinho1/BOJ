   "source": [
    "while 1:\n",
    "    d, rh, rv = map(float, input().split())\n",
    "    if d == rh == rv == 0:\n",
    "        break\n",
    "    w = 16 * d / 337**0.5 \n",
    "    h = w * 9/16\n",
    "    print('Horizontal DPI: %.2f'%(rh/w))\n",
    "    print('Vertical DPI: %.2f'%(rv/h))"
   ]
  },
