   "source": [
    "N = int(input())\n",
    "li = [list(map(int, input().split())) for _ in range(N)]\n",
    "for i in range(1, N):\n",
    "    for j in range(3):\n",
    "        li[i][j] += min(li[i-1][:j]+li[i-1][j+1:])\n",
    "print(min(li[-1]))"
