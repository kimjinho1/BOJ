   "source": [
    "for _ in range(int(input())):\n",
    "    s = input().rstrip()\n",
    "    cnt = 0\n",
    "    for c in s:\n",
    "        if c in \"aeiou\":\n",
    "            cnt += 1\n",
    "    print(f\"The number of vowels in {s} is {cnt}.\")"
   ]
