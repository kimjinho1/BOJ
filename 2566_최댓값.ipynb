    "max_n = max_i = max_j = 0\n",
    "for i in range(1, 10):\n",
    "    li = list(map(int, input().split()))\n",
    "    n = max(li)\n",
    "    if max_n < n:\n",
    "        max_n = n\n",
    "        max_i = i\n",
    "        max_j = li.index(max(li)) + 1\n",
    "print(max_n)\n",
    "print(max_i, max_j)"
