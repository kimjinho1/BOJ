   "source": [
    "N = int(input())\n",
    "li = list(map(int, input().split()))\n",
    "S = int(input())\n",
    "cnt = N\n",
    "\n",
    "for s in li:\n",
    "    if s > S:\n",
    "        cnt += (s-1)//S\n",
    "    elif s == 0:\n",
    "        cnt -= 1\n",
    "print(S*cnt)"
   ]
