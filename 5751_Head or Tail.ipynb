   "source": [
    "while 1:\n",
    "    N = int(input())\n",
    "    if N == 0:\n",
    "        break\n",
    "    li = list(map(int, input().split()))\n",
    "    j = m = 0\n",
    "    for n in li:\n",
    "        if n == 1:\n",
    "            j += 1\n",
    "        else:\n",
    "            m += 1\n",
    "    print(f\"Mary won {m} times and John won {j} times\")"
   ]
