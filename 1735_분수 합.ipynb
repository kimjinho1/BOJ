   "source": [
    "def gcd(a, b):\n",
    "    if b == 0:\n",
    "        return a\n",
    "    a, b = b, a%b \n",
    "    return gcd(a, b)\n",
    "\n",
    "n1, d1 = map(int, input().split())\n",
    "n2, d2 = map(int, input().split())\n",
    "n = n1*d2 + d1*n2\n",
    "d = d1*d2\n",
    "g = gcd(max(n,d), min(n,d))\n",
    "print(n//g, d//g)"
   ]
  },
