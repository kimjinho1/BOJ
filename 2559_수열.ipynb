   "source": [
    "N, K = map(int, input().split())\n",
    "li = list(map(int, input().split()))\n",
    "t = sum(li[:K])\n",
    "ans = t\n",
    "for i in range(K, N):\n",
    "    t += li[i]\n",
    "    t -= li[i-K]\n",
    "    ans = max(ans, t)\n",
    "print(ans)"
   ]
