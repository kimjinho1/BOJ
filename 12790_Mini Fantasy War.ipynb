    "for _ in range(int(input())):\n",
    "    li = list(map(int, input().split()))\n",
    "    s = [li[i] + li[i+4] for i in range(4)]\n",
    "    res = max(s[0], 1) + max(s[1], 1)*5 + max(s[2], 0)*2 + s[3]*2\n",
    "    print(res)"
