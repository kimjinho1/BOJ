   "source": [
    "n = int(input())\n",
    "i = ok = 0\n",
    "while(i < n/3+1):\n",
    "    if (n - 3*i) % 5 == 0:\n",
    "        ok = 1\n",
    "        break\n",
    "    i += 1\n",
    "print((n-3*i)//5+i) if ok else print(-1)"
