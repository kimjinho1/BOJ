   ],
   "source": [
    "for i in range(int(input())):\n",
    "    li = input().split()\n",
    "    res = \"NO\"\n",
    "    if li[1] == '+' and int(li[0])+int(li[2]) == int(li[4]):\n",
    "        res = \"YES\"\n",
    "    if li[1] == '-' and int(li[0])-int(li[2]) == int(li[4]):\n",
    "        res = \"YES\"\n",
    "    print(f\"Case {i+1}: {res}\")"
   ]
