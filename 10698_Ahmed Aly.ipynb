      "4\n",
      "1 + 23 = 22\n",
      "Case 1: NO\n",
      "1 + 22 == 23\n",
      "Case 2: YES\n",
      "22 - 1 = 23\n",
      "Case 3: NO\n",
      "23 - 1 = 22\n",
      "Case 4: YES\n"
     ]
