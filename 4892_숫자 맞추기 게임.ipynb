    "while 1:\n",
    "    n0 = int(input())\n",
    "    if n0 == 0:\n",
    "        break\n",
    "    n1 = 3*n0\n",
    "    n2 = (n1+1)//2 if n1%2 else n1//2\n",
    "    n3 = 3*n2\n",
    "    n4 = n3//9\n",
    "    if n0 == 2*n4:\n",
    "        print(f\"{i}. even {n4}\")\n",
    "    else:\n",
    "        print(f\"{i}. odd {n4}\")\n",
    "    i += 1"
